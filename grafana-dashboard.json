{
  "dashboard": {
    "title": "Contract Intelligence Engine - Service Observability",
    "uid": "contract-intelligence-observability",
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "rate(http_server_duration_count{service_name=\"contract-intelligence-api\"}[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "spanNulls": false,
              "showPoints": "never"
            },
            "unit": "reqps"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "lastNotNull"]
          }
        }
      },
      {
        "id": 2,
        "title": "P95 Latency",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "histogram_quantile(0.95, rate(http_server_duration_bucket{service_name=\"contract-intelligence-api\"}[5m]))",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10,
              "spanNulls": false,
              "showPoints": "never"
            },
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": null,
                  "color": "green"
                },
                {
                  "value": 10,
                  "color": "yellow"
                },
                {
                  "value": 12,
                  "color": "red"
                }
              ]
            }
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "lastNotNull"]
          }
        }
      },
      {
        "id": 3,
        "title": "Queue Depth",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "bullmq_queue_waiting{service_name=\"contract-intelligence-api\"}",
            "legendFormat": "{{queue_name}} - waiting"
          },
          {
            "refId": "B",
            "expr": "bullmq_queue_active{service_name=\"contract-intelligence-api\"}",
            "legendFormat": "{{queue_name}} - active"
          },
          {
            "refId": "C",
            "expr": "bullmq_queue_delayed{service_name=\"contract-intelligence-api\"}",
            "legendFormat": "{{queue_name}} - delayed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20,
              "spanNulls": false,
              "showPoints": "never",
              "stacking": {
                "mode": "normal"
              }
            },
            "unit": "short"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["mean", "max", "lastNotNull"]
          }
        }
      },
      {
        "id": 4,
        "title": "RAGAS Evaluation Scores",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "refId": "A",
            "expr": "ragas_faithfulness_score{service_name=\"legal-agent\"}",
            "legendFormat": "Faithfulness"
          },
          {
            "refId": "B",
            "expr": "ragas_answer_relevancy_score{service_name=\"legal-agent\"}",
            "legendFormat": "Answer Relevancy"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
